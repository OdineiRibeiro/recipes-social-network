<table style="width:100%">
  <tr>
    <th>Id</th>
    <th>Cozinha</th>
    <th></th>
  </tr>

<% @cuisines.each do |cuisine| %>
  <tr>
    <td><%= cuisine.id %></td>
    <td><%= cuisine.name %></td>
    <td><%= link_to 'Editar cozinha', action: 'edit', id: cuisine %></td>
  </tr>
<% end %>
</table>
<%= link_to 'Cadastrar nova cozinha', new_cuisine_path %>
